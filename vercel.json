{
    "version": 2,
    "buildCommand": "tsc",
    "outputDirectory": "dist",
    "nodeVersion": "18.x",  
    "functions": {
      "api/index.ts": {
        "runtime": "@vercel/node@3.0.0"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.ts"
      }
    ]
  }
  